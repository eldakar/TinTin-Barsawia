#CLASS {mowienie} OPEN

#ACTION {^%1 przedstawia sie jako:$}
{
	#line gag;
	#echo {<afa>%1 przedstawia sie jako:<099>};
	#variable {opis_postaci} {%1};
	#variable {przedstawianie} {1}
}
{5}

#ALIAS {/showknown}
{
	#foreach {$znane[]} {name}
	{
		#echo {<afa>$name<faa>|<efa> $znane[$name]<099>}
	};
	#unvariable name
}
{5}

#EVENT {RECEIVED LINE}
{
	#if {$przedstawianie == 1}
	{
		#line gag;
		#echo {<afa>%0<099>};
		#variable {nazwa_postaci} {%0};
		#replace nazwa_postaci {.} {};
		#class mowienie open;
		#variable {znane[${nazwa_postaci}][opis]} {$opis_postaci};
		#map get roomvnum numer_pokoju;
		#variable {znane[${nazwa_postaci}][pokoj]} {$numer_pokoju};
		#unvariable numer_pokoju;
		#unvariable nazwa_postaci;
		#unvariable przedstawianie;
		#unvariable opis_postaci;
		#class mowienie close;
		#class mowienie write mowienie.tin
	}
}

#SUBSTITUTE       {^%1 przedstawia sie jako: %2$}           {<afa>%1 przedstawia sie jako:<bcf>%2<099>}                  {5}
#SUBSTITUTE       {^%1 {mowi|krzyczy|pyta|nuci|mruczy|odpowiada|oznajmia|wola|spiewa|dudni|pokrzykuje} do ciebie:%3$}                                                                          {<afa>%1 %2:<bcf>%3<099>}  {5}
