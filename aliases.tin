}
{5}

#ALIAS {/mapon}
{
	#map read ${nazwa_krainy}_map;
	#if {&{postacie[$nazwa_postaci][ustawienia][room_v]}}
	{
		#map goto ${postacie[$nazwa_postaci][ustawienia][room_v]}
	};
	#else
	{
		#map goto 1;
		#map list {$last_room};
		#map goto {$last_room}
	};
	#variable map_state {1};
	#cr
}
{5}

