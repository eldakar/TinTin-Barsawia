#ALIAS {log}
{
	#if {"%0" == "on"}
	{
		#if {"$logging" != "on"}
		{
			#format {logfile} {${nazwa_krainy}_logs/%t.log} {%d-%m-%Y};
			#log append $logfile;
			#variable logging on
		};
		#else
		{
			#echo Logowanie jest juz aktywne.
		}
	};
	#elseif {"%0" == "off"}
	{
		#if {"$logging" == "on"}
		{
			#log off;
			#variable logging off
		};
		#else
		{
			#echo Logowanie nie jest aktywne.
		}
	};
	#else
	{
		#echo Wlaczyc logowanie czy wylaczyc?
	}
}
